{
  "name": "basketball-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:json": "vitest --reporter=json --outputFile=test-results/vitest-results.json --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --reporter=default --reporter=json --outputFile=test-results/vitest-results.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:json && npm run test:e2e",
    "monitor": "cd scripts && node test-monitor-native.js",
    "watch:tests": "tsx scripts/test-watcher.ts",
    "playwright:install": "playwright install chromium firefox webkit",
    "update:verbaende": "node ../scripts/update-verbaende.js",
    "check:packages": "node ../scripts/check-packages.js",
    "check:all-packages": "node ../scripts/check-all-packages.js",
    "check:unused": "node ../scripts/check-unused.js",
    "update:packages": "npm update && npm audit fix"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "lucide-react": "^0.546.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@pact-foundation/pact": "^16.0.2",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.9.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.11.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.4",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.8",
    "jest-axe": "^10.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
