{
  "name": "basketball-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:json": "vitest --reporter=json --outputFile=test-results/vitest-results.json --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --reporter=default --reporter=json --outputFile=test-results/vitest-results.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:json && npm run test:e2e",
    "monitor": "cd scripts && node test-monitor-native.js",
    "watch:tests": "tsx scripts/test-watcher.ts",
    "playwright:install": "playwright install chromium firefox webkit",
    "update:verbaende": "node ../scripts/update-verbaende.js",
    "check:packages": "node ../scripts/check-packages.js",
    "check:all-packages": "node ../scripts/check-all-packages.js",
    "check:unused": "node ../scripts/check-unused.js",
    "update:packages": "npm update && npm audit fix"
  },
  "dependencies": {
    "dexie": "^4.0.10",
    "lucide-react": "^0.468.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "uuid": "^13.0.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@pact-foundation/pact": "^16.0.2",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.8.6",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.4",
    "globals": "^15.11.0",
    "happy-dom": "^20.0.8",
    "jest-axe": "^8.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.7.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
